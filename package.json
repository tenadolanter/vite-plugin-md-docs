  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/tenadolanter/vite-plugin-md-docs.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tenadolanter/vite-plugin-md-docs/issues"
  },
  "homepage": "https://github.com/tenadolanter/vite-plugin-md-docs#readme",
  "dependencies": {
    "markdown-it": "^14.1.0",
    "markdown-it-anchor": "^8.6.7",
    "markdown-it-chain": "^1.3.0",
    "markdown-it-container": "^4.0.0",
    "transliteration": "^2.3.5"
  },
  "devDependencies": {
    "@vitejs/plugin-vue2": "^2.2.0",
    "@vue/component-compiler-utils": "^2.6.0",
    "element-ui": "^2.15.14",
    "highlight.js": "^11.9.0",
    "sass": "^1.69.5",
